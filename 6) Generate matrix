# Generate the following matrices.
[,1] [,2] [,3] [,4]
[1,] 1 101 201 301
[2,] 2 102 202 302
[3,] 3 103 203 303
[4,] 4 104 204 304
[5,] 5 105 205 305


v <- c(seq(1,5),seq(101,105),seq(201,205),seq(301,305))
matrix(v,5,4)


#Output
> matrix(v,5,4)
     [,1] [,2] [,3] [,4]
[1,]    1  101  201  301
[2,]    2  102  202  302
[3,]    3  103  203  303
[4,]    4  104  204  304
[5,]    5  105  205  305
